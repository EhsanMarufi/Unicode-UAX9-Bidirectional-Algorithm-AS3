package nibblessoft.unicode.data
{
	public final class JoiningType
	{
		/** An invalid Joining Type (value = <code>0</code>)*/
		public static const INVALID:uint = 0;
		
		/** Right_Joining */
		public static const R:uint = 1;
		
		/** Left_Joining */
		public static const L:uint = 2;
		
		/** Dual_Joining */
		public static const D:uint = 3;
		
		/** Join_Causing */
		public static const C:uint = 4;
		
		/** Non_Joining */
		public static const U:uint = 5;
		
		/** Transparent */
		public static const T:uint = 6;
	}
}